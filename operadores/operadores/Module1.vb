        Dim condicion As Boolean = 44 > 6

        Dim condicionAnd As Boolean = 44 > 6 And 3 > 1

        Dim condicionOr As Boolean = 44 > 6 Or 3 > 1

        Dim condicionNot As Boolean = Not 3 < 1

        Console.WriteLine(aritmeticoSuma)

