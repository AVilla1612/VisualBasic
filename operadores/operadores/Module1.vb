Module Module1

    Sub Main()

        Dim aritmeticoSuma As Double = 8 + 3

        Dim aritmeticoResta As Double = 8 - 3

        Dim aritmeticoMulti As Double = 8 * 3

        Dim aritmeticoMod As Double = 8 Mod 3

        Dim aritmeticoIncrement As Double = 8
        aritmeticoIncrement += 2

        Dim aritmeticoDecrement As Double = 8
